{{#if note}}
    <h3>
        {{note.title}}
    </h3>
    <p>
        {{note.body}}
    </p>
{{/if}}
<p>
    Key: {{notekey}}
</p>
{{#if notekey}}
    <hr>
    <p>
        <a href="/notes/destroy?key={{notekey}}"> Delete Note </a> | <a href="/notes/edit?key={{notekey}}"> Edit Note </a>
    </p>
{{/if}}